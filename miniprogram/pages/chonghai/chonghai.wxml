<swiper indicator-dots="true" autoplay="true" interval="5000" duration="200" style="width:100%">
  <block wx:for="{{imgUrls}}" wx:for-item="item">
  <swiper-item>
  <image src="{{item.imgsrc}}" class="slide-image" style="width:100%;height:500rpx;"/>
  </swiper-item>
  </block>
</swiper>

<swiper indicator-dots="true" autoplay="true" interval="6000" duration="500" style="width:100%">
  <block wx:for="{{imgUrl}}" wx:for-item="item">
  <swiper-item>
  <image src="{{item.imgsr}}" class="slide-image" style="width:100%;height:500rpx;"/>
  </swiper-item>
  </block>
</swiper>

<view>
<button bindtap="upLoad" class="upload">选择图片</button>
</view>



